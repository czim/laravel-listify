{
    "name": "czim/laravel-listify",
    "description": "Turns any model into a list. Rebuild of the popular Lookitsatravis Listify package.",
    "keywords": [
        "eloquent",
        "model",
        "list",
        "position"
    ],
    "homepage": "https://github.com/czim/laravel-listify",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pxlwidgets.com",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\Listify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\Listify\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
